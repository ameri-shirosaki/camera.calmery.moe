{
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "styled-components",
        {
          "displayName": false,
          "fileName": false,
          "pure": true,
          "ssr": true
        }
      ]
    ]
  },
  "dependencies": {
    "blueimp-load-image": "2.26.0",
    "isomorphic-unfetch": "3.0.0",
    "next": "9.3.2",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-redux": "7.2.0",
    "redux": "4.0.5",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "styled-components": "5.0.1",
    "uuid": "7.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.8.7",
    "@storybook/addon-knobs": "5.3.17",
    "@storybook/react": "5.3.17",
    "@types/blueimp-load-image": "2.23.5",
    "@types/jest": "25.1.4",
    "@types/node": "13.9.1",
    "@types/react": "16.9.23",
    "@types/react-redux": "7.1.7",
    "@types/react-test-renderer": "16.9.2",
    "@types/redux-logger": "3.0.7",
    "@types/styled-components": "5.0.1",
    "@types/uuid": "7.0.0",
    "@typescript-eslint/eslint-plugin": "2.23.0",
    "@typescript-eslint/parser": "2.23.0",
    "babel-loader": "8.0.6",
    "babel-plugin-styled-components": "1.10.7",
    "babel-preset-react-app": "9.1.1",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "2.5.0",
    "husky": "4.2.3",
    "jest": "25.1.0",
    "lint-staged": "10.0.8",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "react-test-renderer": "16.13.0",
    "ts-jest": "25.2.1",
    "typescript": "3.8.3"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "jest": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended",
      "plugin:react/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint",
      "react",
      "react-hooks"
    ],
    "rules": {
      "@typescript-eslint/explicit-function-return-type": 0
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{ts,tsx}",
      "!<rootDir>/src/**/*.stories.{ts,tsx}"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": {
          "jsx": "react"
        }
      }
    },
    "moduleNameMapper": {
      "^~/(.+)": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "npm run lint:eslint"
    ],
    "**/*.{js,json,jsx,md,yml}": [
      "npm run lint:prettier"
    ]
  },
  "name": "camera.calmery.moe",
  "private": true,
  "scripts": {
    "build": "next build",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint --ignore-path .gitignore --fix . --ext .ts --ext .tsx",
    "lint:prettier": "prettier --ignore-path .gitignore --write \"**/*.{js,json,jsx,md,yml}\"",
    "start": "next dev",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "test": "jest --coverage"
  },
  "version": "0.1.0"
}
