{
  "dependencies": {
    "@sentry/browser": "5.17.0",
    "blueimp-load-image": "5.12.0",
    "next": "9.4.4",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-ga": "3.0.0",
    "react-redux": "7.2.0",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "resize-observer-polyfill": "1.5.1",
    "styled-components": "5.1.1",
    "ua-parser-js": "0.7.21"
  },
  "devDependencies": {
    "@babel/core": "7.10.3",
    "@storybook/addon-knobs": "5.3.19",
    "@storybook/react": "5.3.19",
    "@types/blueimp-load-image": "2.23.7",
    "@types/jest": "26.0.0",
    "@types/node": "14.0.13",
    "@types/react": "16.9.36",
    "@types/react-redux": "7.1.9",
    "@types/react-test-renderer": "16.9.2",
    "@types/redux-logger": "3.0.8",
    "@types/styled-components": "5.1.0",
    "@types/ua-parser-js": "0.7.33",
    "@typescript-eslint/eslint-plugin": "3.2.0",
    "@typescript-eslint/parser": "3.2.0",
    "babel-loader": "8.1.0",
    "babel-plugin-styled-components": "1.10.7",
    "babel-preset-react-app": "9.1.2",
    "eslint": "7.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "lint-staged": "10.2.10",
    "npm-run-all": "4.1.5",
    "prettier": "2.0.5",
    "react-test-renderer": "16.13.1",
    "redux-logger": "3.0.6",
    "ts-jest": "26.1.0",
    "typescript": "3.9.5"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "jest": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended",
      "plugin:react/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    },
    "plugins": [
      "@typescript-eslint",
      "react",
      "react-hooks"
    ],
    "rules": {
      "@typescript-eslint/explicit-function-return-type": 0,
      "@typescript-eslint/explicit-module-boundary-types": 0,
      "react/prop-types": 0
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{ts,tsx}",
      "!<rootDir>/src/**/*.stories.{ts,tsx}"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": {
          "jsx": "react"
        }
      }
    },
    "moduleNameMapper": {
      "^~/(.+)": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "npm run lint:eslint"
    ],
    "**/*.{js,json,jsx,md,yml}": [
      "npm run lint:prettier"
    ]
  },
  "name": "camera.calmery.moe",
  "private": true,
  "scripts": {
    "build": "next build",
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint --ignore-path .gitignore --fix . --ext .ts --ext .tsx",
    "lint:prettier": "prettier --ignore-path .gitignore --write \"**/*.{js,json,jsx,md,yml}\"",
    "start": "next dev",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "test": "jest --coverage"
  },
  "version": "0.1.0"
}
